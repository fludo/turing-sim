<?xml version="1.0" encoding="UTF-8"?>
<turing type="multitape" variant="unbound" name="MultiTape inverter sample" description="Simply invert all tapes os the machine">
	<input alphabet="1,0,#"/>
	
	<states>
		<state label="q0"/>
		<state label="q1"/>
		<state label="q2"/>
	</states>
	<starting state="q0"/>
	<accepting states="q2"/>
	<rejecting states="q1"/>
	
	<tapes>
	  <tape indice="0" alphabet="1,0,#" content="1010010101010010101#">
	    <transitions>
		<transition from_state="q0" tapeSymbol="0" to_state="q1" new_tapeSymbol="1" direction="R"/>
		<transition from_state="q0" tapeSymbol="1" to_state="q1" new_tapeSymbol="0" direction="R"/>
		<transition from_state="q1" tapeSymbol="0" to_state="q1" new_tapeSymbol="1" direction="R"/>
		<transition from_state="q1" tapeSymbol="1" to_state="q1" new_tapeSymbol="0" direction="R"/>
		<transition from_state="q1" tapeSymbol="#" to_state="q2" new_tapeSymbol="#" direction="R"/>
		<transition from_state="q0" tapeSymbol="#" to_state="q2" new_tapeSymbol="#" direction="R"/>
	    </transitions>
	  </tape>
	  <tape indice="1" alphabet="1,0,#" content="1010011111110010101#">
	    <transitions>
		<transition from_state="q0" tapeSymbol="0" to_state="q1" new_tapeSymbol="1" direction="R"/>
		<transition from_state="q0" tapeSymbol="1" to_state="q1" new_tapeSymbol="0" direction="R"/>
		<transition from_state="q1" tapeSymbol="0" to_state="q1" new_tapeSymbol="1" direction="R"/>
		<transition from_state="q1" tapeSymbol="1" to_state="q1" new_tapeSymbol="0" direction="R"/>
		<transition from_state="q1" tapeSymbol="#" to_state="q2" new_tapeSymbol="#" direction="R"/>
		<transition from_state="q0" tapeSymbol="#" to_state="q2" new_tapeSymbol="#" direction="R"/>
	    </transitions>
	  </tape>
	  <tape indice="3" alphabet="1,0,#" content="0000011011110010101#">
	    <transitions>
		<transition from_state="q0" tapeSymbol="0" to_state="q1" new_tapeSymbol="1" direction="R"/>
		<transition from_state="q0" tapeSymbol="1" to_state="q1" new_tapeSymbol="0" direction="R"/>
		<transition from_state="q1" tapeSymbol="0" to_state="q1" new_tapeSymbol="1" direction="R"/>
		<transition from_state="q1" tapeSymbol="1" to_state="q1" new_tapeSymbol="0" direction="R"/>
		<transition from_state="q1" tapeSymbol="#" to_state="q2" new_tapeSymbol="#" direction="R"/>
		<transition from_state="q0" tapeSymbol="#" to_state="q2" new_tapeSymbol="#" direction="R"/>
	    </transitions>
	  </tape>
	</tapes>
</turing>
	
